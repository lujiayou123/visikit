{
  "algo": "MAML",
  "baseline": {
    "$class": "meta_mb.baselines.linear_baseline.LinearFeatureBaseline"
  },
  "discount": 0.99,
  "env": {
    "$class": "meta_mb.meta_envs.mujoco.ant_rand_goal.AntRandGoalEnv"
  },
  "exp_tag": "",
  "exploration": false,
  "gae_lambda": 1,
  "hidden_nonlinearity": {
    "function": "tanh"
  },
  "hidden_sizes": [
    64,
    64
  ],
  "inner_lr": 0.1,
  "inner_type": "log_likelihood",
  "learn_std": true,
  "max_path_length": 200,
  "meta_batch_size": 40,
  "n_itr": 1001,
  "normalize_adv": true,
  "num_inner_grad_steps": 1,
  "output_nonlinearity": null,
  "parallel": true,
  "positive_adv": false,
  "rollouts_per_meta_task": 20,
  "scope": null,
  "seed": 2,
  "step_size": 0.01
}